        "express": "^4.17.1",
        "nodemon": "^2.0.3"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-standard": "^4.0.1",
        "lint-staged": "^10.1.5",
        "prettier": "^2.0.4"
    },
    "lint-staged": {
        "*.js": "eslint --fix",
        "*.+(json|md)": "prettier --write"
